game:GetService("StarterGui"):SetCore("SendNotification", { 
	Title = "Network Reanimation:";
	Text = "For the people. The only reanimation you'll ever need";
	Icon = "rbxthumb://type=Asset&id=712804347&w=150&h=150"})
