                shp(v, "BackendAccoutrementState", 0)
            end 
        end
    end
    anything:Destroy()
    end
 if _G.wackyhead == true and _G.HatBody == true then
     c.Head:Destroy()
     c.PrimaryPart = c["Right Arm"]
     
     end
if _G.CFramePermaMode == true then
game:GetService("RunService").Heartbeat:Connect(function()
   
    if _G.HatLimbs == true then
     c["Kate Hair"].Handle.CFrame = char["Left Arm"].CFrame*CFrame.Angles( math.rad(90),0,  0)
       c["Pal Hair"].Handle.CFrame = char["Right Arm"].CFrame*CFrame.Angles( math.rad(90),0,  0)
     if _G.ShowLegs == false then
             c["Pink Hair"].Handle.CFrame = char["Left Leg"].CFrame*CFrame.Angles( math.rad(90),0,  0)
                  c["LavanderHair"].Handle.CFrame = char["Right Leg"].CFrame*CFrame.Angles( math.rad(90),0,  0)
                  else 
                 c["Right Leg"].CFrame = char["Right Leg"].CFrame
    c["Left Leg"].CFrame = char["Left Leg"].CFrame
    end
   else

    c["Right Leg"].CFrame = char["Right Leg"].CFrame
    c["Left Leg"].CFrame = char["Left Leg"].CFrame
      c["Left Arm"].CFrame = char["Left Arm"].CFrame
      if _G.wackyhead == false then
          c["Right Arm"].CFrame = char["Right Arm"].CFrame
          else
               c["Head"].CFrame = char["Right Arm"].CFrame *CFrame.Angles(0, 0, math.rad(-90))
              end
      end

if _G.fakeHeadMovementv2 == false then
    c["Torso"].CFrame = char["Torso"].CFrame
    else
           c["SeeMonkey"].Handle.CFrame = char["Torso"].CFrame*CFrame.Angles( math.rad(90),0,  0)
        
end
         if _G.wackyhead == false then
if removinghealth == false then
    c["Head"].CFrame = char["Head"].CFrame
end
else
   c["Right Arm"].CFrame = char["Head"].CFrame *CFrame.Angles(0,0,math.rad(90))
    end
 for _,v in pairs(c:GetChildren()) do
       if v:IsA("Accessory") and v.Name ~= "Pal Hair" and v.Name ~= "SeeMonkey" and v.Name ~= "Pink Hair" and v.Name ~= "LavanderHair" and v.Name ~= "Kate Hair"and v.Name ~= "MeshPartAccessory"   then
v.Handle.CFrame = char[v.Name].Handle.CFrame
           end
 end
  for _,v in pairs(c:GetChildren()) do
       if v:IsA("Accessory")   then
v.Handle.CanCollide = false
           end
 end
   for _,v in pairs(c:GetChildren()) do
       if v:IsA("Part") and v.Name ~= "Pal Hair" and v.Name ~= "SeeMonkey" and v.Name ~= "Pink Hair" and v.Name ~= "LavanderHair" and v.Name ~= "Kate Hair" then
v.Color = char[v.Name].Color
           end
   end
   
   

end)

pcall(function()
    if _G.wackyhead == false then
while true do
wait(5)
local posback = char.Head.Position -- change char to the reanim char name
removinghealth = true
c.Head.Position = Vector3.new(999999999999999,999999999999999,999999999999999) -- change c to the real char name
    wait(.1)
    local posback = char.Head.Position
    removinghealth = false
    c.Head.Position = posback
end
end
end)
else
    Align(c.Head, char.Head,Vector3.new(0,0,0),Vector3.new(0,0,0))
      Align(c.Torso, char.Torso,Vector3.new(0,0,0),Vector3.new(0,0,0))
        Align(c["Right Arm"], char["Right Arm"],Vector3.new(0,0,0),Vector3.new(0,0,0))
          Align(c["Left Arm"], char["Left Arm"],Vector3.new(0,0,0),Vector3.new(0,0,0))
              Align(c["Right Leg"], char["Right Leg"],Vector3.new(0,0,0),Vector3.new(0,0,0))
          Align(c["Left Leg"], char["Left Leg"],Vector3.new(0,0,0),Vector3.new(0,0,0))
              for _,v in pairs(c:GetChildren()) do
      
if v:IsA("Accessory") then
   Align(v.Handle, char:FindFirstChild(v.Name).Handle,Vector3.new(0,0,0),Vector3.new(0,0,0))
           end
       end

    pcall(function()
while true do
wait(5)
local posback = char.Head.Position -- change char to the reanim char name
removinghealth = true
unalign(c.Head)
c.Head.Position = Vector3.new(999999999999999,999999999999999,999999999999999) -- change c to the real char name
    wait(.1)
    removinghealth = false
    c.Head.Position = posback
       realign(c.Head)
       end
end)
    end

          
local function play(id, volume)
    if volume == nil then
        volume = default_volume
    end
    local sound = Instance.new("Sound")
    sound.SoundId = id
    sound.Volume = volume
    game.SoundService:PlayLocalSound(sound)
end




